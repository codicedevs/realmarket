{
  "expo": {
    "name": "Real Market Inversiones",
    "slug": "real-market-inversiones",
    "version": "2.1",
    "orientation": "portrait",
    "icon": "./assets/rm-negro.png",
    "scheme": "realmarket",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/rm-negro.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codice.real-market-inversiones",
      "buildNumber": "2.1"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Lato/Lato-Regular.ttf",
            "./assets/fonts/Lato/Lato-Bold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c7655900-1729-4406-8096-96427a396e58"
      }
    },
    "owner": "widoski",
    "android": {
      "package": "com.widoski.realmarketinversiones"
    }
  }
}
