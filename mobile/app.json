{
  "expo": {
    "name": "realmarket",
    "slug": "realmarket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rm-negro.png",
    "scheme": "realmarket",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codice.real-market-argentina-2024"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mativant.realmarket",
      "cleartTextTrafficermitted": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Lato/Lato-Regular.ttf",
            "./assets/fonts/Lato/Lato-Bold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3e867b21-b50a-42fa-ac1e-d0556c759f8e"
      }
    }
  }
}
