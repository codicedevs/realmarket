{
  "expo": {
    "name": "realmarket",
    "slug": "realmarket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rm-negro.png",
    "scheme": "realmarket",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/rm-negro.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codice.real-market-argentina-2024"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mativant.realmarket",
      "cleartTextTrafficermitted": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {}
    }
  }
}
